<Window x:Class="Diff.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Diff"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBlock x:Name="comparedText" HorizontalAlignment="Left" Margin="28,120,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="437">
        </TextBlock>
        <Button Content="Compare" HorizontalAlignment="Left" Margin="169,248,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <TextBox x:Name="firstText" Height="23" Margin="0,39,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" HorizontalContentAlignment="Stretch"/>
        <TextBox x:Name="secondText" Height="23" Margin="0,76,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" HorizontalContentAlignment="Stretch"/>

    </Grid>
</Window>
